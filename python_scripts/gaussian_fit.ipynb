    "import numpy as np\n",
    "\n",
    "def gaussian_plusline(x,mu,sig,A,alpha,beta):\n",
    "    return alpha+x*beta+A*np.exp(-(x-mu)**2/2/sig**2)\n",
    "def gaussian(x,mu,sig,A):\n",
    "    return A*np.exp(-(x-mu)**2/2/sig**2)"
